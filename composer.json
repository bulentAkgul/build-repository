{
  "name": "bakgul/repository-builder",
  "description": "This is the package that will build repository.",
  "authors": [
    {
      "name": "Bulent Akgul",
      "email": "bulent.akgul@me.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "bakgul/kernel": "^4.0",
    "bakgul/file-content": "^4.0",
    "bakgul/file-history": "^4.0",
    "bakgul/laravel-file-creator": "^4.0",
    "bakgul/laravel-package-generator": "^4.0",
    "bakgul/laravel-resource-creator": "^4.0",
    "bakgul/laravel-helpers": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Bakgul\\BuildRepo\\": "src/",
      "Bakgul\\BuildRepo\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bakgul\\BuildRepo\\BuildRepoServiceProvider"
      ]
    }
  }
}